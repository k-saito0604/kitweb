                            <option value="営業関連" >営業関連問い合わせ</option>
                            <option value="技術関連" >技術関連問い合わせ</option>
                            <option value="その他" >その他のお問い合わせ</option>
                          </select--> 
                        </div>

                            <div class="form-group">

                              <label for="message">{{ i18n "contactMessage" }}</label>
                                <textarea id="message" class="form-control" name="message" required></textarea>




                            </div>
                        </div>
                        <span class="form-error" id="form-type_error"></span><br />
                        <center><div data-netlify-recaptcha="true"></div></center>
                        <div class="col-sm-12">
                          {{ .Content }}
