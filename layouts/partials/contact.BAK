                <div id="contact-message"></div>


                  <form name="kitform" method="post" action="/success.html" data-netlify="true">
                    <div class="row">
                            <div class="form-group">
                              <label for="name">{{ i18n "contactName" }}*</label>
                              <span class="form-error" id="form-name_error"></span>
                                <input  type="text" class="form-control" name="name" id="name" required>
                              <label for="furigana">{{ i18n "contactFuri" }}</label>
                                <input  type="furigana" class="form-control" name="firigana" id="firigana">
                              <label for="company">{{ i18n "company" }}*</label>
                                <input  type="company" class="form-control" name="company" id="company" required>
                               <label for="email">{{ i18n "contactMail" }}*</label>
                                <input type="email" class="form-control" name="email" id="email" required>
                            
                              <!--<label for="telephone">{{ i18n "contactTEL" }}</label>
                              <input type="telephone" class="form-control" name="telephone" id="telephone" -->
                          
                         <!--<label class="type" for="form-type">お問い合わせ種類</label>
                          <select class="form-control" id="form-type" name="form-type">
